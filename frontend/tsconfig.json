{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",  // Ajoutez cette ligne
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "types": ["react", "react-dom", "vite/client"],  // Ajoutez vite/client
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}